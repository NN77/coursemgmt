{
  "name": "coursemgmt",
  "version": "1.0.0",
  "description": "A simple SPA app consuming a REST API built with AngularJS, Angular Material, PostCss, Jade, Gulp",
  "repository": {
    "type": "git",
    "url": "https://github.com/NN77/coursemgmt.git"
  },
  "bugs": {
    "url": "https://github.com/NN77/coursemgmt/issues"
  },
  "scripts": {
    "postinstall": "npm start",
    "prestart": "npm update",
    "start": "gulp serve.dev --color",
    "serve.dev": "gulp serve.dev --color",
    "test": "karma start karma.conf.js",
    "test-single-run": "karma start karma.conf.js --single-run"
  },
  "private": true,
  "author": "Hubert Gudan <hubert.gudan@kinguin.net>",
  "peerDependencies": {
    "angular": "@>=1.3 <1.6",
    "angular-animate": "^@>=1.3 <1.6",
    "angular-aria": "@>=1.3 <1.6",
    "angular-messages": "@>=1.3 <1.6"
  },
  "devDependencies": {
    "gulp": "^3.9.1",
    "gulp-concat": "^2.6.0",
    "gulp-postcss": "^6.1.1",
    "gulp-pug": "^3.0.3",
    "gulp-shell": "^0.5.2",
    "gulp-traceur": "^0.17.2",
    "jasmine-core": "^2.4.1",
    "karma": "^0.13.22",
    "karma-chrome-launcher": "^0.2.3",
    "karma-firefox-launcher": "^0.1.7",
    "karma-jasmine": "^0.3.8",
    "karma-junit-reporter": "^0.4.1",
